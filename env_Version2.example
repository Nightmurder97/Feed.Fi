# Feed.Fi Environment Configuration
# Copy this file to .env and fill in your API keys

# Gemini AI API Key (required for AI features)
# Get your key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=tu_clave_gemini

# Gemini 2.5 Flash Configuration
GEMINI_MODEL=models/gemini-2.5-flash-preview-05-20
GEMINI_MAX_TOKENS=8192
GEMINI_TEMPERATURE=0.3
GEMINI_USE_THINKING=true

# Optional: Other AI providers (for future use)
# OPENAI_API_KEY=your_openai_key_here
# HUGGINGFACE_API_KEY=your_huggingface_key_here

# App Configuration
APP_NAME=Feed.Fi
BUNDLE_ID=com.feedfi.app

# Development Settings
DEBUG_AI_LOGS=true
AI_REQUEST_TIMEOUT=60